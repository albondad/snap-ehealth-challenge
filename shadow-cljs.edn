{:source-paths ["src"]
 :dependencies [[binaryage/devtools "1.0.2"]
                [nrepl "0.8.3"]
                [reagent "1.0.0"]]
 :jvmp-opts ["Xmx1G"]
 :builds       {:app {:target     :browser
                      :output-dir "public/js"
                      :asset-path "/js"
                      :modules    {:app {:entries [snap-ehealth-challenge.core]}}
                      :devtools   {:after-load snap-ehealth-challenge.core/mount-root}}}

 :dev-http     {3000 {:root    "public"
                      :handler user/app}}}
